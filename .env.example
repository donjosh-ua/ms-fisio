# Database Configuration
POSTGRES_DB=database_name
POSTGRES_USER=database_user
POSTGRES_PASSWORD=database_password

# JWT Configuration
JWT_SECRET=mySecretKeyForJWTTokenGenerationInContainer
JWT_ACCESS_EXPIRATION=86400000  # 1 day in milliseconds
JWT_REFRESH_EXPIRATION=604800000  # 7 days in milliseconds

# Application Configuration
SPRING_PROFILE=docker
SERVER_PORT=8080

# External Services (for production)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:4200,http://localhost:8081
